{
    "name": "codemastersolutions/code-acl",
    "description": "Pacote para Controle de Acesso/Autorização",
    "keywords": [
        "codemaster",
        "solutions",
        "laravel",
        "permission",
        "permissions",
        "roles",
        "acl",
        "security"
    ],
    "type": "package",
    "homepage": "https://github.com/codemastersolutions/code-acl",
    "license": "MIT",
    "authors": [
        {
            "name": "Gilson Gabriel",
            "email": "gilson@codemastersolucoes.com",
            "homepage": "https://codemastersolucoes.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodeMaster\\CodeAcl\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CodeMaster\\CodeAcl\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeMaster\\CodeAcl\\CodeAclServiceProvider"
            ],
            "aliases": {}
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "codemastersolutions/code-log": "^1.0",
        "illuminate/auth": "^9.0",
        "illuminate/container": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/database": "^9.0",
        "rebing/graphql-laravel": "^8.2",
        "spatie/laravel-sluggable": "^3.4"
    },
    "require-dev": {
        "orchestra/testbench": "^7.3",
        "phpunit/phpunit": "^9.5",
        "predis/predis": "^1.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
