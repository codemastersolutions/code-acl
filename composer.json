{
    "name": "codemastersolutions/code-acl",
    "description": "Pacote para Controle de Acesso/Autorização",
    "keywords": [
        "codemaster",
        "solutions",
        "laravel",
        "permission",
        "permissions",
        "roles",
        "acl",
        "security"
    ],
    "type": "package",
    "homepage": "https://github.com/codemastersolutions/code-acl",
    "license": "MIT",
    "authors": [
        {
            "name": "Gilson Gabriel",
            "email": "gilson@codemastersolucoes.com",
            "homepage": "https://codemastersolucoes.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodeMaster\\CodeAcl\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CodeMaster\\CodeAcl\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeMaster\\CodeAcl\\CodeAclServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "codemastersolutions/code-log": "^0.1.1",
        "illuminate/auth": "^8.6",
        "illuminate/container": "^8.6",
        "illuminate/contracts": "^8.6",
        "illuminate/database": "^8.6",
        "rebing/graphql-laravel": "^5.1",
        "spatie/laravel-sluggable": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^6.2",
        "phpunit/phpunit": "^9.3",
        "predis/predis": "^1.1",
        "spatie/phpunit-watcher": "^1.22"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
